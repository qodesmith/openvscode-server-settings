{
  "name": "openvscode-server-settings",
  "displayName": "openvscode-server-settings",
  "description": "Start openvscode-server with initial settings",
  "version": "0.0.1",
  "publisher": "Qodesmith",
  "repository": "https://github.com/qodesmith/openvscode-server-settings",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vscode/vsce": "^2.26.1",
    "eslint": "^8.26.0",
    "typescript": "^5.4.2"
  }
}
